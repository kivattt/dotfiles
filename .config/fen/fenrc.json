{
	"dont-show-hidden-files": true,
	"open-with": [
		{
			"programs": ["mpv", "vlc"],
			"match": [
				"*.mp4", "*.webm", "*.mkv", "*.mov", "*.avi", "*.flv",
				"*.wav", "*.flac", "*.mp3", "*.ogg", "*.m4a"
			]
		},
		{
			"programs": ["xviewer", "xdg-open", "feh"],
			"match": ["*.png", "*.jpg", "*.jpeg", "*.jfif", "*.flif", "*.tiff", "*.gif", "*.webp", "*.bmp"]
		},
		{
			"programs": ["nvim -p", "vim -p", "vi -p"],
			"match": ["*"]
		}
	],
	"preview-with": [
		{
			"script": "file-previews/markdown.lua",
			"match": ["*.md"]
		},
		{
			"script": "file-previews/rainbow.lua",
			"match": ["*"],
			"do-not-match": [
				"*.mp4", "*.webm", "*.mkv", "*.mov", "*.avi", "*.flv",
				"*.wav", "*.flac", "*.mp3", "*.ogg", "*.m4a",

				"*.png", "*.jpg", "*.jpeg", "*.jfif", "*.flif", "*.tiff", "*.gif", "*.webp", "*.bmp",

				"*.o", "*.otf", "*.appimage",

				"*.zip", "*.jar", "*.kra",
				"*.tar.bz2", "*.tb2", "*.tbz", "*.tbz2", "*.tz2",
				"*.tar.gz", "*.taz", "*.tgz",
				"*.tar.lz",
				"*.tar.lzma", "*.tlz",
				"*.tar.lzo",
				"*.tar.xz", "*.tZ", "*.taZ",
				"*.tar.zst", "*.tzst",
			
				"fen"
			]
		},
		{
			"programs": ["file -b"],
			"match": ["*"]
		}
	]
}
