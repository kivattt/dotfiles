! Old config
!xterm*selectToClipboard: true
!xterm*font:              *-fixed-*-*-*-10-*
!xterm*background:black
!xterm*foreground:white

! Properties
XTerm.termName: xterm-256color
XTerm.vt100.selectToClipboard: true
XTerm.vt100.saveLines: 4096
! Disables Alt+Enter going fullscreen
XTerm.fullscreen: never
! Don't know if this makes a difference, but it should enable UTF-8 if it wasn't already I suppose
XTerm.vt100.locale: false
XTerm.vt100.utf8: true

! Fixes backspace in fen, ^- is undef (https://invisible-island.net/xterm/manpage/xterm.html#Application-Resources:ttyModes)
XTerm.ttyModes: erase ^-

! special
*.foreground:   #cad0d1
*.background:   #101018
*.cursorColor:  #cad0d1

! black
*.color0:       #000000
*.color8:       #6b6b6b

! red
*.color1:       #742636
*.color9:       #e03e60

! green
*.color2:       #1b710e
*.color10:      #00ef6b

! yellow
*.color3:       #d28b52
*.color11:      #fff041

! blue
*.color4:       #4272a1
*.color12:      #0a9df6

! magenta
*.color5:       #783c8a
*.color13:      #e247c6

! cyan
*.color6:       #479fac
*.color14:      #00e5ff

! white
*.color7:       #788085
*.color15:      #ffffff

! Font
XTerm.vt100.faceName: JetBrains Mono:size=9:antialias=true
XTerm.vt100.renderFont: true

! X11
Xcursor.size: 16
!*dpi: 125
!Xft.dpi: 125
